alias n="nvim"
alias nu="nvim -u 'no-config'"
alias lg="git lg"
alias st="git st"
alias nn='nvim -c "Notes"'
alias nni='nvim ~/vimwiki/index.md'
alias nnd='nvim -c "Diary"'
alias dot='nvim ~/dotfiles/$(cd ~/dotfiles && git ls-files | fzf)'
alias nrc='nvim ~/dotfiles/nvim/.config/nvim/lua/plugins.lua'
alias nnr="nvim -c 'NNR'"
alias py="python3"
alias ls='ls -G'
alias ll='ls -l'
alias l='ls -ltrh'
alias lsd='ls -d "$PWD"/*'
alias kp='xargs -I _ kedropipe _'
alias pp='pwd | pbcopy'
alias m='make'
alias ma='cat Makefile'
alias nG='n -c ":G"'
alias nd='n -c ":DiffviewOpen"'
alias re='exec zsh'
alias hsr='hs -c "hs.reload()"'
alias tw='timew'
alias tws='timew start'
alias twst='timew stop'
alias k='kubectl'
alias env_add='set -a && source .env && set +a'
alias gb='gh browse --branch $(git branch --show-current)'
alias prv='gh pr view --web'
alias gbl='lab repo browse || gh browse'
alias j='fasd_cd -d'
alias v='f -e nvim'
alias vv='f -e vim'
alias pyenv_init='eval "$(pyenv init -)"'
alias "??"="gh copilot suggest -t shell"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
